<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   android:paddingBottom="10dp">


    <TextView
            android:id="@+id/wylot_z"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintTop_toTopOf="@+id/view3" />

    <TextView
            android:id="@+id/wylot_z2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintTop_toBottomOf="@+id/przylot_do"
            android:layout_marginTop="44dp"/>
    <TextView
            android:id="@+id/data_wylotu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginTop="10dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#4800D0"
            app:layout_constraintStart_toEndOf="@+id/wylot_z"
            app:layout_constraintTop_toTopOf="@+id/view3" />

    <TextView
            android:id="@+id/data_wylotu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#4800D0"
            app:layout_constraintStart_toEndOf="@+id/wylot_z2"
            app:layout_constraintTop_toBottomOf="@+id/data_przylotu" android:layout_marginTop="44dp"
            android:layout_marginStart="25dp"/>
    <TextView
            android:id="@+id/przylot_do"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="152dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintTop_toBottomOf="@+id/wylot_z"
            app:layout_constraintVertical_bias="1.0"/>

    <TextView
            android:id="@+id/przylot_do2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintTop_toBottomOf="@+id/wylot_z2"
            android:layout_marginTop="48dp"/>
    <TextView
            android:id="@+id/data_przylotu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginBottom="10dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#4800D0"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintStart_toEndOf="@+id/przylot_do"
            app:layout_constraintTop_toBottomOf="@+id/data_wylotu" app:layout_constraintVertical_bias="0.26"/>

    <TextView
            android:id="@+id/data_przylotu2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#4800D0"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintStart_toEndOf="@+id/przylot_do2"
            app:layout_constraintTop_toBottomOf="@+id/data_wylotu2" app:layout_constraintVertical_bias="0.8"
            android:layout_marginStart="25dp"/>
    <TextView
            android:id="@+id/cena"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotobold"
            android:text="TextView"
            android:textColor="#4800D0"
            app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintVertical_bias="0.454" android:textSize="24sp"/>

    <TextView
            android:id="@+id/przewoznik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintTop_toTopOf="@+id/view3"
            app:layout_constraintBottom_toTopOf="@+id/cena" app:layout_constraintStart_toEndOf="@+id/przesiadki"
            android:layout_marginStart="48dp" app:layout_constraintVertical_bias="0.511"/>
    <TextView
            android:id="@+id/przewoznik2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintStart_toEndOf="@+id/przesiadki"
            android:layout_marginStart="48dp" app:layout_constraintTop_toBottomOf="@+id/cena"
            app:layout_constraintBottom_toBottomOf="@+id/view3"/>
    <TextView
            android:id="@+id/przesiadki"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintVertical_bias="0.19"/>
    <TextView
            android:id="@+id/przesiadki2"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:elevation="7dp"
            android:fontFamily="@font/robotoregular"
            android:text="TextView"
            android:textColor="#000000"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/data_wylotu2"
            app:layout_constraintBottom_toBottomOf="@+id/view3" app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintEnd_toStartOf="@+id/przewoznik2" app:layout_constraintHorizontal_bias="0.737"
            app:layout_constraintVertical_bias="0.018"/>
    <View
            android:id="@+id/view3"
            android:layout_width="330dp"
            android:layout_height="250dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/bg_flights_row"
            android:elevation="6dp"
            android:padding="5dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>